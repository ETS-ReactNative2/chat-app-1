option_settings:
  aws:elbv2:listener:80:
    DefaultProcess: http
    ListenerEnabled: 'true'
    Protocol: HTTP
    Rules: static,media
  aws:elbv2:listenerrule:static:
    PathPatterns: /static/*
    Process: webserver
    Priority: 1
  aws:elbv2:listenerrule:media:
    PathPatterns: /media/*
    Process: webserver
    Priority: 2
  aws:elasticbeanstalk:environment:process:http:
    Port: '8000'
    Protocol: HTTP
  aws:elasticbeanstalk:environment:process:webserver:
    Port: '80'
    Protocol: HTTP